<h1>Sign up</h1>

<%= form_with model: @user, url: registrations_path do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Sign Up" %>
  </div>
<% end %>
